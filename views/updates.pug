doctype html
html 
    head 
        title= title
        meta(name="viewport" content="width=device-width, initial-scale=1")
        link(rel="stylesheet", href="/css/style.css")
    body    
        h1 Update Custom Object
        a(href='/') Return to Homepage

        .form-wrapper
            form(method="POST")
                label(for="newName") Name 
                input(type="text" name="newName" required)

                label(for="newDesc") Description 
                textarea( name="newDesc" required rows="5")

                label(for="newDate") Date 
                input(type="text" name="newDate" required placeholder="YYYY-MM-DD")
                
                input(type="submit" value="Submit")